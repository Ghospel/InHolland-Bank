		{
			"name": "get token",
			"event": [
				{
					"listen": "test",
					"script": {
						"id": "ddcec026-bab0-43c6-864e-1ccb0e898585",
						"exec": [
							"pm.test(\"Status code is 200\", function () { pm.response.to.have.status(200); });",
							"",
							"pm.test(\"Access token is present\", function () {",
							"    var jsonData = pm.response.json();",
							"    pm.expect(jsonData.access_token).to.not.eql(undefined);",
							"});",
							"pm.test(\"Response time is less than 200ms\", function () {",
							"    pm.expect(pm.response.responseTime).to.be.below(200);",
							"});",
							"pm.test(\"Token type is bearer\", function () {",
							"    var jsonData = pm.response.json();",
							"    pm.expect(jsonData.token_type).to.eql(\"bearer\");",
							"});",
							"pm.test(\"Expiration is not longer than 5000\", function () {",
							"    var jsonData = pm.response.json();",
							"    pm.expect(jsonData.expires_in).to.be.below(5001);",
							"});"
						],
						"type": "text/javascript"
					}
				}
			],
